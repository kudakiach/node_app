<head>
    <link rel="stylesheet" href="/stylesheets/style.css">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js"></script>
</head>

<body>

    <div class="center">
        <div style="    padding: 10px 300px;">
            <h1>{{ title }}</h1>
            <p class="profile">LOGGED IN AS: {{ username }}</p>

            <form method="get" action="/logout" class="logout">
                <button type="submit" class="btn btn-block btn-danger">Logout</button>
            </form>



            <form method="post" action="/dashboard" id="myForm" style="width: 100%;">
                <div class='form-field' style="width: 80%; float:left">
                    <select id='imageFilter' class='form-control' name="id">
                        <option> Choose an image</option>}
                        {{#each anyArray as |val key|}}
                        <option value="{{val}}">{{ val }}</option>
                        {{/each}}
                    </select>
                </div>

                <input type="submit" class="btn btn-success float-right" value="Submit" style="width:20%">

            </form>


            {{#each images as |val key|}}
            <div style="margin-top: 50px">
                <img src="{{val}}" style="height: 425px">
            </div>
            {{/each}}

        </div>


    </div>
</body>